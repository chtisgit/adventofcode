	$(CXX) $(CFLAGS) $(CXXFLAGS) -o $@ $^

4: 4.cpp
	$(CXX) $(CFLAGS) $(CXXFLAGS) -o $@ $^

8: 8.cpp
	$(CXX) $(CFLAGS) $(CXXFLAGS) -o $@ $^

9: 9.cpp
	$(CXX) $(CFLAGS) $(CXXFLAGS) -o $@ $^

clean:
	rm -f 1 2 3 4 8 9
